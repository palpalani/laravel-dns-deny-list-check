{
    "name": "palpalani/laravel-dns-deny-list-check",
    "description": "checks if the given IP address or hostname is blacklisted on the configured dnsbl servers",
    "keywords": [
        "palpalani",
        "laravel-dns-deny-list-check",
        "sending-ip",
        "email"
    ],
    "homepage": "https://github.com/palpalani/laravel-dns-deny-list-check",
    "license": "MIT",
    "authors": [
        {
            "name": "palPalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "palPalani\\LaravelDnsDenyListCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "palPalani\\LaravelDnsDenyListCheck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "palPalani\\LaravelDnsDenyListCheck\\LaravelDnsDenyListCheckServiceProvider"
            ],
            "aliases": {
                "LaravelDnsDenyListCheck": "palPalani\\LaravelDnsDenyListCheck\\LaravelDnsDenyListCheckFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
